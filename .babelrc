{
  "presets": [
    "flow",
    "react",
    [
      "es2015",
      {
        "loose": true,
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": ["react-flow-props-to-prop-types", "transform-decorators-legacy"],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "plugins": [
        "transform-runtime",
        "ramda",
        "lodash"
      ]
    }
  }
}
